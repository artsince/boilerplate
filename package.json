{
  "name": "webapp-boilerplate",
  "description": "Isomorphic React Web App Boilerplate",
  "version": "1.0.0",
  "private": true,
  "main": "app.js",
  "scripts": {
    "test": "karma start",
    "lint": "eslint ./server/*.js ./src/*.jsx ./src/**/*.jsx",
    "build": "browserify -t [ babelify --presets [ es2015 react ] ] src/client.jsx -o dist/bundle.js",
    "start": "node server/app.js"
  },
  "author": "Ahmet Kizilay",
  "repository": {
    "url": "https://github.com/artsince/webapp-boilerplate",
    "type": "git"
  },
  "license": "MIT",
  "dependencies": {
    "babel": "^6.0.15",
    "babel-core": "^6.1.2",
    "express": "^4.13.3"
    "lodash": "^3.10.1",
    "morgan": "^1.6.1",
    "react": "^0.14.2",
    "react-dom": "^0.14.2"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.4",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "eslint": "^1.9.0",
    "eslint-plugin-react": "^3.8.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.0",
    "karma-chrome-launcher": "^0.2.1",
    "karma-jasmine": "^0.3.6",
    "react-addons-test-utils": "^0.14.2"
  },
  "keywords": [
    "babel",
    "babelify",
    "boilerplate",
    "browserify",
    "es2015",
    "eslint",
    "express",
    "jasmine",
    "karma",
    "react",
    "webapp"
  ],
  "engines": {
    "node": ">=4.2.1"
  }
}
